{
	"auto_complete": false,
	"auto_match_enabled": false,
	"color_scheme": "Writer.sublime-color-scheme",
	"fold_buttons": false,
	"folder_exclude_patterns":
	[
		".git",
		"node_modules"
	],
	"font_face": "JetBrains Mono",
	"font_size": 16,
	"hot_exit": "disabled",
	"ignored_packages":
	[
		"Vintage",
	],
	"save_on_focus_lost": true,
	"tab_size": 2,
	"theme": "auto",
	"translate_tabs_to_spaces": true,
}
